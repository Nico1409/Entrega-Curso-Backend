<div>
  <table class="styled-table">
    <thead>
      <tr>
        <th>Title</th>
        <th>Description</th>
        <th>Code</th>
        <th>Price</th>
        <th>Status</th>
        <th>Stock</th>
        <th>Category</th>
        <th>ID</th>
      </tr>
    </thead>
    <tbody>
      {{#each products}}
      <tr>
        <td>{{title}}</td>
        <td>{{description}}</td>
        <td>{{code}}</td>
        <td>{{price}}</td>
        <td>{{status}}</td>
        <td>{{stock}}</td>
        <td>{{category}}</td>
        <td>{{id}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
<script src="./js/index.js"></script>
